using System;

namespace Cycle3
{
    class Program
    {
        static void Main(string[] args)
        {
            int currentNumber;
            int firstNumber = 5;
            int addNumber = 7;
            int maxRange = 100;

            for (currentNumber = firstNumber ; currentNumber < maxRange; currentNumber += addNumber)
            {
                Console.WriteLine(currentNumber);
            }

        }
    }
}

