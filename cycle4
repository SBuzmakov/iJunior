using System;

namespace Cycles4
{
    class Program
    {
        static void Main(string[] args)
        {
            int randomNumber;
            int minRange = 1;
            int maxRange = 20;
            Random rand = new Random();
            randomNumber = rand.Next(minRange, maxRange);
            int currentNumber;
            int multNumber1 = 3;
            int sumNumber;

            Console.WriteLine($" Random number is: {randomNumber}");


            for (currentNumber = minRange; currentNumber <= randomNumber; currentNumber++ ) // список всех чисел для наглядности
            {
                Console.WriteLine(currentNumber);
            }

            Console.WriteLine($"Multiple numbers of {multNumber1}: ");

            for (currentNumber = minRange; currentNumber <= randomNumber; currentNumber++) // список чисел кратных 3 для наглядности
            {
                if (currentNumber % multNumber1 == 0 )
                {
                    Console.WriteLine(currentNumber);
                }
            }

        }
    }
}
